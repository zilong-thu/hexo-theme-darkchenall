<%
var str = '';
var icon = '<i class="fa fa-file-text"></i> ';
if (item.link)
{
    str += '<a href="'  + item.link + '" target="_blank">';
    if (item.title){
	   str += item.title;
    }
    else
	str += item.link;
    str += "</a>";
}
else if (index)
    str = '<a href="' + config.root + item.path + '">' + icon + item.title + '</a>';
else
    str = icon + item.title;
-%>
<%- str -%>